{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "src",
    "src/**/*.ts",
    "src/**/*.tsx",
    "expo-env.d.ts",
    "expo-router.config.js",
    "nativewind-env.d.ts",
    ".expo/types/**/*.ts"
  ]
}
